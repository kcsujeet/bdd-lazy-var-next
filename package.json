{
  "name": "bdd-lazy-var-next",
  "version": "0.0.1",
  "author": "Sujeet Kc",
  "description": "Provides \"ui\" for testing frameworks such as mocha/jasmine/jest/vitest/bun which allows to define lazy variables and subjects",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:kcsujeet/bdd-lazy-var-next.git"
  },
  "keywords": [
    "mocha.js",
    "jasmine",
    "jest",
    "vitest",
    "bun",
    "bun:test",
    "bdd",
    "lazy",
    "variable",
    "syntax",
    "dsl",
    "subject",
    "rspec"
  ],
  "bugs": {
    "url": "https://github.com/kcsujeet/bdd-lazy-var-next/issues"
  },
  "main": "./dist/index.js",
  "peerDependencies": {
    "jasmine": ">=2",
    "jasmine-core": ">=2",
    "jest": ">=20",
    "mocha": ">=2.3",
    "vitest": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "jasmine": {
      "optional": true
    },
    "jasmine-core": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "mocha": {
      "optional": true
    },
    "vitest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "chai": "^4.0.0",
    "chai-spies": "^1.0.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^9.1.7",
    "jasmine": "^4.0.0",
    "jest": "^30.0.0",
    "karma": "^6.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^5.0.0",
    "karma-mocha": "^2.0.0",
    "lint-staged": "^16.2.7",
    "mocha": "^11.0.0",
    "puppeteer": "^24.0.0",
    "semantic-release": "^24.0.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "mocha": "NODE_PATH=. mocha -r src/test/config src/test/*_examples.js src/test/shared_behavior.spec.js",
    "test.mocha-ui": "bun run mocha -- -u dist/index.js src/dialects/bdd.spec.js",
    "test.mocha-global": "bun run mocha -- -u dist/global.js src/dialects/global.spec.js",
    "test.mocha-getter": "bun run mocha -- -u dist/getter.js src/dialects/getter.spec.js",
    "test.mocha": "bun run test.mocha-ui && bun run test.mocha-global && bun run test.mocha-getter",
    "test.browser-mocha-ui": "karma start -u bdd-lazy-var-next --src dist/index.js --specs src/dialects/bdd.spec.js tools/karma.config.js",
    "test.browser-mocha-global": "karma start -u bdd-lazy-var-next/global --src dist/global.js --specs src/dialects/global.spec.js,src/dialects/bdd.spec.js tools/karma.config.js",
    "test.browser-mocha-getter": "karma start -u bdd-lazy-var-next/getter --src dist/getter.js --specs src/dialects/getter.spec.js,src/dialects/bdd.spec.js tools/karma.config.js",
    "test.mocha-in-browser": "bun run test.browser-mocha-ui && bun run test.browser-mocha-global && bun run test.browser-mocha-getter",
    "test.jasmine-ui": "bun run src/features/jasmine/runner.js dist/index.js bdd.spec.js",
    "test.jasmine-global": "bun run src/features/jasmine/runner.js dist/global.js global.spec.js",
    "test.jasmine-getter": "bun run src/features/jasmine/runner.js dist/getter.js getter.spec.js",
    "test.jasmine": "bun run test.jasmine-ui && bun run test.jasmine-global && bun run test.jasmine-getter",
    "test.browser-jasmine-ui": "karma start -f jasmine --src dist/index.js --specs src/dialects/bdd.spec.js tools/karma.config.js",
    "test.browser-jasmine-global": "karma start -f jasmine --src dist/global.js --specs src/dialects/global.spec.js,src/dialects/bdd.spec.js tools/karma.config.js",
    "test.browser-jasmine-getter": "karma start -f jasmine --src dist/getter.js --specs src/dialects/getter.spec.js,src/dialects/bdd.spec.js tools/karma.config.js",
    "test.jasmine-in-browser": "bun run test.browser-jasmine-ui && bun run test.browser-jasmine-global && bun run test.browser-jasmine-getter",
    "test.jest-ui": "SRC_FILE=dist/index.js jest --findRelatedTests src/dialects/bdd.spec.js",
    "test.jest-global": "SRC_FILE=dist/global.js jest --findRelatedTests src/dialects/global.spec.js",
    "test.jest-getter": "SRC_FILE=dist/getter.js jest --findRelatedTests src/dialects/getter.spec.js",
    "test.jest": "bun run test.jest-ui && bun run test.jest-global && bun run test.jest-getter",
    "test.vitest-ui": "SRC_FILE=dist/index.js vitest run src/dialects/bdd.spec.js",
    "test.vitest-global": "SRC_FILE=dist/global.js vitest run src/dialects/global.spec.js",
    "test.vitest-getter": "SRC_FILE=dist/getter.js vitest run src/dialects/getter.spec.js",
    "test.vitest": "bun run test.vitest-ui && bun run test.vitest-global && bun run test.vitest-getter",
    "test.bun-ui": "bun test --preload ./src/features/bun/preloads/ui.js src/dialects/bdd.spec.js",
    "test.bun-global": "bun test --preload ./src/features/bun/preloads/global.js src/dialects/global.spec.js",
    "test.bun-getter": "bun test --preload ./src/features/bun/preloads/getter.js src/dialects/getter.spec.js",
    "test.bun": "bun run test.bun-ui && bun run test.bun-global && bun run test.bun-getter",
    "test": "bun run test.mocha && bun run test.mocha-in-browser && bun run test.jasmine && bun run test.jasmine-in-browser && bun run test.jest",
    "prebuild": "bun run lint",
    "build": "bun run tools/build.js",
    "lint": "eslint --fix src",
    "lint:check": "eslint src",
    "ci": "bun run lint:check && bun run build",
    "ci:test": "bun run ci && bun run test.mocha && bun run test.jasmine && bun run test.vitest && bun run test.bun",
    "prepare": "husky",
    "prerelease": "NODE_ENV=production bun run build",
    "release": "semantic-release"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/dialects/**/*.spec.js"
    ],
    "setupFilesAfterEnv": [
      "./src/features/jest/setup.js"
    ]
  },
  "files": [
    "dist/getter.js",
    "dist/getter.js.map",
    "getter.d.ts",
    "dist/global.js",
    "dist/global.js.map",
    "global.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "index.d.ts",
    "interface.d.ts"
  ],
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "chore",
              "scope": "deps",
              "release": "patch"
            },
            {
              "type": "docs",
              "scope": "README",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
